@using RentSmart.Web.ViewModels.Properties.ViewModels.AllByUser
@model UserAllPropertiesViewModel
@{
    ViewData["Title"] = "My Properties";
}

<div> 
    <div class="container mt-4">
        <h3 class="text-center">@ViewData["Title"]</h3>
    </div>
    <hr />
    <div>
        <partial name="_LikedPartial" model="@Model.LikedProperties" />
    </div>
    <div>
        <partial name="_ManagersPartial" model="@Model.ManagedProperties" />
    </div>
    <div class="container mt-4">
    <partial name="_OwnersPartial" model="@Model.OwnedProperties" />
    </div>
    <div class="container mb-5">
    <partial name="_RentersPartial" model="@Model.RentedProperties" />
    </div> 
</div>